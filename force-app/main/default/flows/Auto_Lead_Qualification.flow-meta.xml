<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <description>Automatically qualify leads when they are created or updated</description>
    <label>Auto Lead Qualification</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>BuilderVersion</name>
        <value>
            <stringValue>240.0</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Qualify_Lead_With_AI</targetReference>
        </connector>
    </start>
    <status>Draft</status>
    <subflows>
        <name>Qualify_Lead_With_AI</name>
        <label>Qualify Lead With AI</label>
        <locationX>50</locationX>
        <locationY>200</locationY>
        <processType>Subflow</processType>
        <subflowProcessType>AutoLaunchedFlow</subflowProcessType>
    </subflows>
    <triggers>
        <name>Lead_After_Insert_Update</name>
        <label>Lead After Insert/Update</label>
        <events>
            <name>afterInsert</name>
            <object>Lead</object>
        </events>
        <events>
            <name>afterUpdate</name>
            <object>Lead</object>
        </events>
        <filterLogic>1 AND 2</filterLogic>
        <filters>
            <field>Lead.Status</field>
            <operator>equals</operator>
            <value>
                <stringValue>New</stringValue>
            </value>
        </filters>
        <filters>
            <field>Lead.Email</field>
            <operator>isNotNull</operator>
        </filters>
    </triggers>
</Flow>
